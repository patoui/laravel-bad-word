{
    "name": "patoui/laravel-bad-word",
    "description": "Laravel bad word validation rule",
    "keywords": [
        "patoui",
        "laravel",
        "validation",
        "bad",
        "word",
        "profanity"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrique Ouimet",
            "email": "patrique.ouimet@gmail.com",
            "homepage": "https://patriqueouimet.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/validation": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "~6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Patoui\\LaravelBadWord\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patoui\\LaravelBadWord\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patoui\\LaravelBadWord\\BadWordServiceProvider"
            ]
        }
    }
}
